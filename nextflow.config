params {
    input = "data/*.gen"      // Default input glob for .gen files
    reference_panel = "path/to/reference_panel.vcf.gz" // Path to reference panel
    outdir = "results"        // Output directory
    min_maf = 0.01            // Minimum Minor Allele Frequency for QC
    imputation_type = "genotype"
    imputation_method = "priler" // Default imputation method (e.g., impute5, custom_r)
}